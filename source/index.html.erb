---
title: Valentin Ferron - Portfolio
---

<div class="content">

  <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">

    <div class="page-title-content">
      <h2>Valentin Ferron</h2>
      <p>webdesigner</p>
    </div>

		<div class="grid">
      <% data.projects1.each do |project| %>
        <figure class="effect-oscar">
					<img src="<%= project.picture %>" alt=""/>
					<figcaption>
						<h2><%= project.name %><span><%= project.bold %></span></h2>
						<p><%= project.description %></p>
						<a href="#">View more</a>
					</figcaption>
				</figure>
      <% end %>
    </div>
  </div>

  <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
		<div class="grid">
      <% data.projects2.each do |project| %>
        <figure class="effect-oscar">
					<img src="<%= project.picture %>" alt=""/>
					<figcaption>
						<h2><%= project.name %><span><%= project.bold %></span></h2>
						<p><%= project.description %></p>
						<a href="#">View more</a>
					</figcaption>
				</figure>
      <% end %>
    </div>
  </div>

</div>
